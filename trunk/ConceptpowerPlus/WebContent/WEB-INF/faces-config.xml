<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<navigation-rule>
		<display-name>LoginView</display-name>
		<from-view-id>/LoginView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/LoginView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>LoginView</display-name>
		<from-view-id>/LoginView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/auth/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/auth/*</from-view-id>
		<navigation-case>
			<from-outcome>conceptLists</from-outcome>
			<to-view-id>/auth/ConceptLists.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/auth/*</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/LoginView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptLists.xhtml</display-name>
		<from-view-id>/auth/ConceptLists.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewList</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ConceptSearch.xhtml</display-name>
		<from-view-id>/ConceptSearch.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>conceptSearch</from-outcome>
			<to-view-id>/ConceptSearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ConceptSearch.xhtml</display-name>
		<from-view-id>/ConceptSearch.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/LoginView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>LoginView</display-name>
		<from-view-id>/LoginView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>conceptSearch</from-outcome>
			<to-view-id>/ConceptSearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/auth/*</from-view-id>
		<navigation-case>
			<from-outcome>conceptTypes</from-outcome>
			<to-view-id>/auth/TypesListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>/auth/TypesListView.xhtml</display-name>
		<from-view-id>/auth/TypesListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addType</from-outcome>
			<to-view-id>/auth/TypeAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/TypeAddView.xhtml</display-name>
		<from-view-id>/auth/TypeAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/TypesListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/TypeAddView.xhtml</display-name>
		<from-view-id>/auth/TypeAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/auth/TypeAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/auth/*</from-view-id>
		<navigation-case>
			<from-outcome>conceptSearch</from-outcome>
			<to-view-id>/ConceptSearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptAddView.xhtml</display-name>
		<from-view-id>/auth/ConceptAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptAddView.xhtml</display-name>
		<from-view-id>/auth/ConceptAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/auth/ConceptAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/auth/*</from-view-id>
		<navigation-case>
			<from-outcome>manageUsers</from-outcome>
			<to-view-id>/auth/UserListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/UserListView.xhtml</display-name>
		<from-view-id>/auth/UserListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addUser</from-outcome>
			<to-view-id>/auth/UserAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/UserAddView.xhtml</display-name>
		<from-view-id>/auth/UserAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/auth/UserAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/UserAddView.xhtml</display-name>
		<from-view-id>/auth/UserAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/UserListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/UserListView.xhtml</display-name>
		<from-view-id>/auth/UserListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/auth/UserListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptLists.xhtml</display-name>
		<from-view-id>/auth/ConceptLists.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addList</from-outcome>
			<to-view-id>/auth/ListAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ListAddView.xhtml</display-name>
		<from-view-id>/auth/ListAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptLists.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ListAddView.xhtml</display-name>
		<from-view-id>/auth/ListAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/auth/ListAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptLists.xhtml</display-name>
		<from-view-id>/auth/ConceptLists.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newconcept</from-outcome>
			<to-view-id>/auth/ConceptAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptListView.xhtml</display-name>
		<from-view-id>/auth/ConceptListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editConcept</from-outcome>
			<to-view-id>/auth/ConceptEditView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptEditView.xhtml</display-name>
		<from-view-id>/auth/ConceptEditView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>/auth/TypesListView.xhtml</display-name>
		<from-view-id>/auth/TypesListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editType</from-outcome>
			<to-view-id>/auth/TypeEditView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/TypeEditView.xhtml</display-name>
		<from-view-id>/auth/TypeEditView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/auth/TypeEditView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/TypeEditView.xhtml</display-name>
		<from-view-id>/auth/TypeEditView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/TypesListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptLists.xhtml</display-name>
		<from-view-id>/auth/ConceptLists.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newWNconceptwrapper</from-outcome>
			<to-view-id>/auth/ConceptWNWrapperAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptWNWrapperAddView.xhtml</display-name>
		<from-view-id>/auth/ConceptWNWrapperAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptLists.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptListView.xhtml</display-name>
		<from-view-id>/auth/ConceptListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>showConceptDetails</from-outcome>
			<to-view-id>/auth/ConceptView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptView.xhtml</display-name>
		<from-view-id>/auth/ConceptView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addWNConceptToConcept</from-outcome>
			<to-view-id>/auth/WNConceptToWrapperAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/WNConceptToWrapperAddView.xhtml</display-name>
		<from-view-id>/auth/WNConceptToWrapperAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/WNConceptToWrapperAddView.xhtml</display-name>
		<from-view-id>/auth/WNConceptToWrapperAddView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/auth/WNConceptToWrapperAddView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptView.xhtml</display-name>
		<from-view-id>/auth/ConceptView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewList</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptEditView.xhtml</display-name>
		<from-view-id>/auth/ConceptEditView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewList</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptListView.xhtml</display-name>
		<from-view-id>/auth/ConceptListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptListView.xhtml</display-name>
		<from-view-id>/auth/ConceptListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>deleteConcept</from-outcome>
			<to-view-id>/auth/ConceptDeleteView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptDeleteView.xhtml</display-name>
		<from-view-id>/auth/ConceptDeleteView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewList</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ConceptDeleteView.xhtml</display-name>
		<from-view-id>/auth/ConceptDeleteView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/ConceptListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/UserListView.xhtml</display-name>
		<from-view-id>/auth/UserListView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>resetPassword</from-outcome>
			<to-view-id>/auth/ResetPasswordView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ResetPasswordView.xhtml</display-name>
		<from-view-id>/auth/ResetPasswordView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/auth/UserListView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auth/ResetPasswordView.xhtml</display-name>
		<from-view-id>/auth/ResetPasswordView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/auth/ResetPasswordView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!--  validators -->
	 <validator>
        <validator-id>uniqueUsernameValidator</validator-id>
        <validator-class>edu.asu.conceptpower.web.validators.UniqueUsernameValidator</validator-class>
    </validator>
	 <validator>
	 	<description></description>
	 	<display-name>Password Validator</display-name>
	 	<validator-id>passwordValidator</validator-id>
	 	<validator-class>edu.asu.conceptpower.web.validators.PasswordValidator</validator-class>
	 </validator>

</faces-config>
